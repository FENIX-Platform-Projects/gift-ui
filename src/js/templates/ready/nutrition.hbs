{{#each elements}}

    <h3>{{title}}</h3>
    {{#each indicators}}
        <dl class="dl-horizontal">
            <dt class="indicator-title">Indicator</dt>
            <dd>{{indicator}}</dd>
            <dt class="indicator-title">Gender</dt>
            <dd>{{Gender}}</dd>
            <dt class="indicator-title">Age range</dt>
            <dd>{{Age_range}}</dd>
            <dt class="indicator-title">Survey</dt>
            <dd>{{Survey}}</dd>
        </dl>

        <div class="row">
            <div class="col-xs-6">
                <div id="table-energy-intake"></div>
            </div>
            <div class="col-xs-6">
                <div id="pie-energy-intake"></div>
            </div>

        </div><br><br>

    {{/each}}
    <div class="row">
        <div class="col-xs-12">
            <p class="indicator-title">Comments</p>

            <p class="indicator-value">{{comments}}</p>
        </div>
    </div>
    <hr><br>
{{/each}}