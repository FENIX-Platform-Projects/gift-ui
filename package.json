{
  "name": "gift-ui",
  "version": "1.0.1",
  "description": "Repo for GIFT-UI controller",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "echo \"Error: please specify a valid section to develop [e.g. npm run dev:analysis]\" && exit 1",
    "dev:charts": "SECTIONS=charts NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "w-dev:charts": "SET SECTIONS=charts&&SET NODE_ENV=develop&&webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:map": "SECTIONS=map NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "w-dev:mde": "SET SECTIONS=mde&&SET NODE_ENV=develop&&webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:mde": "SECTIONS=mde NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:readyToUse": "SECTIONS=readyToUse NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:base": "SECTIONS=base NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:data_management": "SECTIONS=data_management NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "w-dev:data_management": "SET SECTIONS=data_management&&SET NODE_ENV=develop&&webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "build": "SECTIONS=base,analysis,data_management NODE_ENV=production webpack --display-error-details ",
    "build:base": "SECTIONS=base NODE_ENV=production webpack --display-error-details ",
    "build:map": "SECTIONS=map NODE_ENV=production webpack --display-error-details ",
    "build:stats": "SECTIONS=base,analysis,data_management NODE_ENV=production webpack --display-error-details --profile --json  > stats.json",
    "w-build": "SET SECTIONS=base,analysis,data_management&&SET NODE_ENV=production&&webpack --display-error-details",
    "w-build:stats": "SET SECTIONS=base,analysis,data_management&&SET NODE_ENV=production&&webpack --display-error-details --profile --json  > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FENIX-Platform-Projects/gift-ui.git"
  },
  "author": "FENIX team (http://fenix.fao.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FENIX-Platform-Projects/gift-ui/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "fenix-ui-bridge": "^1.0.0-beta.9",
    "fenix-ui-catalog": "^2.0.0-beta.22",
    "fenix-ui-data-management": "^2.0.0-beta.65",
    "fenix-ui-filter": "^3.0.0-beta.42",
    "fenix-ui-map": "^2.6.5",
    "highcharts": "^5.0.3",
    "jquery": "^3.1.1",
    "leaflet": "^0.7.7",
    "loglevel": "^1.4.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "jshint": "^2.9.3",
    "jshint-loader": "^0.8.3",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.15.1"
  }
}
